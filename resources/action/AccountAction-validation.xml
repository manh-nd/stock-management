<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN" 
"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="account.fullname">
		<field-validator type="requiredstring">
			<message key="errors.account.fullname" />
		</field-validator>
	</field>
	<field name="account.username">
		<field-validator type="requiredstring">
			<message key="errors.account.username" />
		</field-validator>
	</field>
	<field name="account.password">
		<field-validator type="requiredstring">
			<message key="errors.account.password" />
		</field-validator>
	</field>
	<field name="account.role">
		<field-validator type="requiredstring">
			<message key="errors.account.role" />
		</field-validator>
	</field>
	<field name="account.report">
		<field-validator type="requiredstring">
			<message key="errors.account.report" />
		</field-validator>
	</field>
</validators>